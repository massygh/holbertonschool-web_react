{
    "compilerOptions": {
      "outDir": "./dist/",
      "rootDir": "./",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "allowJs": true,
      "skipLibCheck": true
    },
    "include": ["./js/**/*.ts"],
    "exclude": ["node_modules"]
  }